{
  "name": "ts-transformer-svg-jsx",
  "version": "0.3.3",
  "description": "Embed SVG files as JSX in TypeScript files.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "files": [
    "dist",
    "transformer.js"
  ],
  "scripts": {
    "build": "tsc-multi",
    "watch": "npm run build -- --watch",
    "lint": "eslint . --ext .js,.ts",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/ts-transformer-svg-jsx.git"
  },
  "keywords": [
    "typescript",
    "lodash",
    "transform"
  ],
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tommy351/ts-transformer-svg-jsx/issues"
  },
  "homepage": "https://github.com/tommy351/ts-transformer-svg-jsx#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "svg-parser": "^2.0.4",
    "svgo": "^2.3.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/svg-parser": "^2.0.1",
    "@types/svgo": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "ts-jest": "^26.5.4",
    "tsc-multi": "^0.5.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "typescript": "*"
  }
}
